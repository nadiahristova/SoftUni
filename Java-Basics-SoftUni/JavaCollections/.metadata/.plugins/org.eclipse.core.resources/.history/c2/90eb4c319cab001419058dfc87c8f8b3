public class dfhdhdh {
	public static void main(String[] args) throws IOException {		
		Scanner input = new Scanner(System.in);
		String line = null, text = null;
		do {
			line = input.next();
			text += line + " ";
			String lastChar = "" + line.charAt(line.length()-1);
			boolean boo = line.matches("[.]+");
		} while (!("" + line.charAt(line.length()-1)).equals("\r"));
		//String line = input.nextLine();
		//String text = input.nextLine();
		String[] words = text.trim().split("\\W+");
		//String[] words = text.trim().split("[^a-zA-z]+");
		System.out.println(words.length);
	}	
}
